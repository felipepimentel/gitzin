{
  "name": "gitzin",
  "displayName": "Gitzin",
  "description": "Easily manage Git and generate automatic commit messages",
  "version": "0.0.1",
  "publisher": "seu_nome_ou_organização",
  "repository": {
    "type": "git",
    "url": "https://github.com/seu_usuario/seu_repositorio.git"
  },
  "license": "MIT",
  "keywords": [
    "git",
    "commit",
    "message",
    "automation"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "assets/icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitzin.generateCommitMessage",
        "title": "Generate Commit Message"
      }
    ],
    "views": {
      "scm": [
        {
          "type": "webview",
          "id": "commitMessageView",
          "name": "Commit Message"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.5",
    "@types/glob": "^8.1.0",
    "@types/json-schema": "^7.0.15",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.16.12",
    "@types/sinon": "^10.0.15",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "sinon": "^15.1.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.4.5"
  }
}
